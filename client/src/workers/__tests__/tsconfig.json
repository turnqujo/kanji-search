{
  "extends": "../../../tsconfig.json",
  "include": ["./*.spec.ts"],
  "exclude": [],
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": ["webworker", "ES6"],
    "module": "None",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": false,
    "strict": true,
    "resolveJsonModule": false,
    "target": "ES6"
  }
}
